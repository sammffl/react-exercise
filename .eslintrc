{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "globals": {
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": 0, //缩进
    "one-var": 0, //是否定义在一行
    "react/prop-types": 2, //是否指定proptypes
    "no-use-before-define": 1, //是否可以提前使用
    "react/jsx-no-bind": 2,
    "max-len": 0,
//    "object-curly-spacing":2, //object里面的空格
    "no-unused-expressions": 0,  //a && fn()
    "camelcase": 1, //驼峰命名
    "react/sort-comp": 0, //方法排序
    "react/prefer-stateless-function": 0,
    "strict": 1,          //严格模式
    "no-nested-ternary": 1,   //嵌套的三目运算
    "react/jsx-indent-props": [1, 4], // jsx 缩进,
    "react/jsx-indent": [1, 4], // jsx 缩进,
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "arrow-body-style": [2, "as-needed"],
    "no-console": 0,
    "import/no-unresolved": 0,
    "no-param-reassign": [2, { "props": false }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  },
  "extends": "airbnb"
}
